Damping=1e-5

!**** PARAMETERS ************************************************************

! comment out following lines in batch mode
!WIDTH=3
!HEIGHT=0.5
!LHEIGHT=0.05
!CDEPTH=0.25
!CSIZE=0.207894736842
!CPOSITION=1.5
!NN=55

! Variables
CWIDTH=CSIZE/500
CENTER=WIDTH/2
DIVISIONS=100

! Base
ex1=70e9
nu1=.34
dens1=2700

! Coating
ex2=210e9
nu2=.28
dens2=7700

!**** MATERIAL PROPERTIES *****************************************************
